{
  "name": "ideas2life/cron-jobby",
  "description": "A rapper for cronjobs using jobby for scheduling",
  "type": "library",
  "license": "GPL-3.0-or-later",
  "version": "0.0.2",
  "authors": [
    {
      "name": "xatzistnr",
      "email": "george@ideas2life.com.cy"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "hellogerard/jobby": "^3.4",
    "ideas2life/i2log": "~0.0.3"
  },
  "config": {
    "platform": {
      "php": "5.6"
    }
  },
  "autoload": {
    "psr-4": {
      "Ideas2life\\CronJobby\\": "src/"
